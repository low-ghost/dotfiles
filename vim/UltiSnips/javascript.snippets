endsnippet

snippet obj-red "Reduce an object"
Object.keys(${1:obj}).reduce(
	(acc, ${2:nextKey}) => {
		
		const ${3:value} = $1[$2];
    $0

		return acc;
		
	},
	${2:seed}
);
endsnippet

pre_expand "wrap_to_eol(snip, 'Object.keys(', 2)"
snippet ok "Object.keys()"
$1);
endsnippet

